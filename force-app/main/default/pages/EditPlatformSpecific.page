                <apex:inputText styleClass="Lev_{!platformSpecific.Id}" value="{!platformSpecific.Level1_Name__c}" id="level1Name"  style="background-color: rgb(236, 235, 234);" onmouseover="Hidelevel('pageId:frm:PB:PBS2:level1Name')" onmouseout="showlevel('pageId:frm:PB:PBS2:level1Name')"/>
               
                  <!--<apex:inputField value="{!platformSpecific.Level_2_record_ID__c}" id="level2Id" />-->
