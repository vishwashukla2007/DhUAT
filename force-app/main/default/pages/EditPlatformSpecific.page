                        <label style="vertical-align: top;"> Level 2 ID </label>
                       <input style="vertical-align: top;" class="opp2_{!platformSpecific.Id}" type="text" id="LeveltwoId" name="name" value="{!platformSpecific.Level_2_Record_ID__r.Account_ID__c}" disabled="true" onchange="Level2Function()"/>  
