                   <apex:outputPanel id="yearPnl">
                     <apex:selectList value="{!platformSpecific.Specialty_Pharmacy_Benefit_Choice__c}" label="Speciality Pharmacy benifit Choice" id="SpecialityPharmacybenifitChoice" size="1">
                       <apex:selectOptions value="{!pharmacyBenefitChoiceValues}" />
