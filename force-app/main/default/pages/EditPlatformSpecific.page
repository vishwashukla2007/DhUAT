                <apex:inputText styleClass="Lev2_{!platformSpecific.Id}" value="{!platformSpecific.Level2_Name__c}" id="level2Name" style="background-color: rgb(236, 235, 234);" onmouseover="Hidelevel('pageId:frm:PB:PBS2:level2Name')" onmouseout="showlevel('pageId:frm:PB:PBS2:level2Name')" />
                
