                <apex:outputPanel layout="inline">  
                  <div class="requiredInput" id="lookupcontainer"  style="text-align: right;margin-right: 60px;">  
                        <label style="vertical-align: top;">Level 3 ID </label>
                       <input style="vertical-align: top;" class="opp3_{!platformSpecific.Id}" type="text" name="name" id="levlthreeId" value="{!platformSpecific.Level_3_record_ID__r.Group_ID__c}" disabled="true" onchange="Level3Function()"/>  
