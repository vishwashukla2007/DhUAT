    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" />
    
    <style type="text/css">
        .dateFormat{
           visibility:hidden;
        }
         .message ul,.messageText{
           
            font-weight: bold;
        }
        </style>
    
     <script>
     try{
     
        window.onload=function(){
        
            var nsfSectionRequired = '{!nsfSectionRequired}';
            var sfSectionRequired = '{!sfSectionRequired}';
            var gstpSectionRequired = '{!gstpSectionRequired}';            
            var dGSTPFormularyRequired = '{!dGSTPFormularyRequired}';
            var showTermflag = '{!showTermflag}';
            
            var level1ID = document.getElementById('{!$Component.frm.PB.PBS2.level1Id}');
            var level2ID = document.getElementById('{!$Component.frm.PB.PBS2.level2Id}');
            var level3ID = document.getElementById('{!$Component.frm.PB.PBS2.level3Id}');
            var effectiveDateID = document.getElementById('{!$Component.frm.PB.PBS3.effectiveDateID}');
            if(showTermflag == 'true'){
            var terminatedID = document.getElementById('{!$Component.frm.PB.PBS3.terminatedID}');
            var terminatedDateID = document.getElementById('{!$Component.frm.PB.PBS3.terminatedDateID}');
            }
            
            if(gstpSectionRequired == 'true'){
            var GSTPEffectiveDateID = document.getElementById('{!$Component.frm.PB.PBS4.GSTPEffectiveDateID}');
            var GSTPEndDateId = document.getElementById('{!$Component.frm.PB.PBS4.GSTPEndDateId}');
            var GSTPChoiceID = document.getElementById('{!$Component.frm.PB.PBS4.GSTPChoiceID}');
            }
            
            if(sfSectionRequired == 'true'){
            var sfEffectiveDateId = document.getElementById('{!$Component.frm.PB.PBS5.sfEffectiveDateId}');
            var sfEndDateId = document.getElementById('{!$Component.frm.PB.PBS5.sfEndDateId}');
           // var benefitChoiceID = document.getElementById('{!$Component.frm.PB.PBS3.benefitChioceID}');
            var sfChoiceId = document.getElementById('{!$Component.frm.PB.PBS5.sfChoiceId}');
            }
            
            if(nsfSectionRequired == 'true'){
            var nsfEffectiveDateId = document.getElementById('{!$Component.frm.PB.PBS6.nsfEffectiveDateId}');
            var nsfRebateEndDateId = document.getElementById('{!$Component.frm.PB.PBS6.nsfRebateEndDateId}');
            var nsfChoiceId = document.getElementById('{!$Component.frm.PB.PBS6.nsfChoiceId}');
            }
            
            
            
            //var GSTPFormularyID = document.getElementById('{!$Component.frm.PB.PBS3.GSTPFormularyID}');
                                    
            var clientSubType = '{!clientSubType}';
            var termFlag = '{!platformSpecific.Termed__c}'; 
            var diffGSTPFlag = '{!platformSpecific.DifferentGSTPFormularyEffectiveDates__c}';
            
            if(diffGSTPFlag == 'false'){
               if(gstpSectionRequired == 'true'){
                   GSTPEffectiveDateID.disabled = true;
                   GSTPEndDateId.disabled = true;
               }    
               if(sfSectionRequired == 'true'){
                   sfEffectiveDateId.disabled = true;
                   sfEndDateId.disabled = true;
               }
               if(nsfSectionRequired == 'true'){
                   nsfEffectiveDateId.disabled = true;
                   nsfRebateEndDateId.disabled = true;
               }
            }
            
            
            /* Commented to populate GSTP and effective dates PT US 157 by Srini Bathini
              if(nsfSectionRequired == 'false'){
                  if(gstpSectionRequired == 'true'){
                   GSTPEffectiveDateID.disabled = true;
                    GSTPChoiceID.disabled = true;
               }  
               if(sfSectionRequired == 'true'){
                   sfEffectiveDateId.disabled = true;
                    sfChoiceId.disabled = true;
               }
                 if(nsfSectionRequired == 'true'){
                   nsfEffectiveDateId.disabled = true;
                  //  nsfChoiceId.disabled = true;
               }                        
             }*/
            
            var clientChoice = '{!clientChoice}';
            
            if(clientChoice == 'false'){
               if(sfSectionRequired == 'true'){
                //  benefitChoiceID.disabled = true;
               }
            }
            
            var rClientChoice = '{!rClientChoice}';
            
            /* Commented to populate GSTP and effective dates PT US 157 by Srini Bathini
            if(rClientChoice == 'false'){
               if(gstpSectionRequired == 'true'){
                   GSTPChoiceID.disabled = true;
               }
               if(sfSectionRequired == 'true'){
                   sfChoiceId.disabled = true;
               }
               
               if(nsfSectionRequired == 'true'){
                   var nsfCoiceId = document.getElementById('{!$Component.frm.PB.PBS6.nsfCoiceId}');
                   nsfCoiceId.disabled = true;
               }
            } */
            
            if(clientSubType == 'Health Plan Master' || clientSubType == 'Coalition Master' || clientSubType == 'TPA Master'){
                effectiveDateID.disabled = false;
                if(showTermflag == 'true'){
                    terminatedID.disabled = false;
                    terminatedDateID.disabled = false;
                }
                }
            if(termFlag == 'false'){
                if(showTermflag == 'true'){
                    terminatedDateID.disabled = true;
                    terminatedDateID.value = '';
                }
                }    
            if(clientSubType != 'Health Plan Master' && clientSubType != 'Coalition Master' && clientSubType != 'TPA Master'){
                 effectiveDateID.disabled = true;  
                //benefitChoiceID.disabled = true;  
               // GSTPFormularyID.disabled = true; 
               if(showTermflag == 'true'){
                   terminatedID.disabled = true;  
                 }        
                }    
                     
           
        }
          
       
        
        function level1Check(level1Id){
             
            var level1Value = document.getElementById(level1Id).value;
            var level1ID = document.getElementById('{!$Component.frm.PB.PBS2.level1Id}');
            
            var level2ID = document.getElementById('{!$Component.frm.PB.PBS2.level2Id}');
            var level3ID = document.getElementById('{!$Component.frm.PB.PBS2.level3Id}');
            
            if(level1Value!= null && level1Value!= ''){
            
               var theAccountId = document.getElementById('{!$Component.frm.PB.PBS2.level1Id}_lkid').value;
              
               var platformValue = document.getElementById('{!$Component.frm.PB.PBS2.platform}');
               level2ID.disabled= false;
               if(level2ID.value){
                   level3ID.disabled= false; 
                  }else{                             
                            level3ID.disabled= true;                            
                         } 
               if(platformValue.value == 'RxCLAIM'){
                   checkLevel1(level1Value);     
                 }
              }else{
                      level2ID.disabled= true; 
                      level3ID.disabled= true;
                      checkLevel1(level1Value);
                   }            
        };
        
        function level2Check(level2Id){
             
            var level2Value = document.getElementById(level2Id).value;
            var level1ID = document.getElementById('{!$Component.frm.PB.PBS2.level1Id}');
            var level2ID = document.getElementById('{!$Component.frm.PB.PBS2.level2Id}');
            var level3ID = document.getElementById('{!$Component.frm.PB.PBS2.level3Id}');
            
            if(level2Value!= null && level2Value!= ''){
               var platformValue = document.getElementById('{!$Component.frm.PB.PBS2.platform}');
               level3ID.disabled= false;
               if(platformValue.value == 'RxCLAIM'){
                   checkLevel2(level2Value);     
                 }
              }else{
                      level3ID.disabled= true; 
                      checkLevel2(level2Value);
                   }            
        };
        
        function level3Check(level3Id){
             
            var level3Value = document.getElementById(level3Id).value;
            if(level3Value!= null && level3Value!= ''){
               var platformValue = document.getElementById('{!$Component.frm.PB.PBS2.platform}');
               if(platformValue.value == 'RxCLAIM'){
                   checkLevel3(level3Value);     
                 }
              }else{
                      checkLevel3(level3Value);
                   }            
        };
        
        function effectiveDateCheck(effectiveDateID){
             
            var effectiveDate = document.getElementById(effectiveDateID).value;
            
             if(effectiveDate){
               checkEffectiveDate(effectiveDate); 
            }else{
                    checkEffectiveDate(effectiveDate); 
                 }             
        };
        
        function termFlagCheck(checkbox){  
                    
            terminatedDateCheck(checkbox.checked);
        };
        
        function terminateDateCheck(terminateDateID){  
                    
            var terminateDate= document.getElementById(terminateDateID).value;
            if(terminateDate){
               checkTerminateDate(terminateDate); 
            }else{
                    checkTerminateDate(terminateDate); 
                 }
        };
        
        function terminateDateCheck(terminateDateID){  
                    
            var terminateDate= document.getElementById(terminateDateID).value;
            if(terminateDate){
               checkTerminateDate(terminateDate); 
            }else{
                    checkTerminateDate(terminateDate); 
                 }
        };
        
        function GSTPFormularyDatesCheck(checkbox){  
           
            gstpFormularyDatesCheck(checkbox.checked);
            
                
        };
        
        function sfEndDateCheck(sfEndDateId){  
          
           var sfEndDate = document.getElementById(sfEndDateId).value;
            
            var sfEffectiveDate = document.getElementById('{!$Component.frm.PB.PBS5.sfEffectiveDateId}').value;
            
            if(sfEndDate){
               sfEndDateCheckaction(sfEffectiveDate,sfEndDate); 
            }else{
                    sfEndDateCheckaction(sfEffectiveDate,sfEndDate); 
                 }
           
        };
        
        function nsfEndDateCheck(nsfRebateEndDateId){  
          
           var nsfEndDate = document.getElementById(nsfRebateEndDateId).value;
          
            var nsfEffectiveDate = document.getElementById('{!$Component.frm.PB.PBS6.nsfEffectiveDateId}').value;
          
            if(nsfEndDate){
               nsfEndDateCheckaction(nsfEffectiveDate,nsfEndDate); 
            }else{
                    nsfEndDateCheckaction(nsfEffectiveDate,nsfEndDate); 
                 }
           
        };
        
        function gstpEndDateCheck(GSTPEndDateId){  
         
           var gstpEndDate = document.getElementById(GSTPEndDateId).value;
          
            var gstpEffectiveDate = document.getElementById('{!$Component.frm.PB.PBS4.GSTPEffectiveDateID}').value;
         
            if(gstpEndDate){
               gstpEndDateCheckaction(gstpEffectiveDate,gstpEndDate); 
            }else{
                    gstpEndDateCheckaction(gstpEffectiveDate,gstpEndDate); 
                 }
           
        };
        
        function sfEffectiveDateCheck(sfEffectiveDateId){  
        
           var sfEffectiveDate = document.getElementById(sfEffectiveDateId).value;
         
            var effectiveDate = document.getElementById('{!$Component.frm.PB.PBS3.effectiveDateID}').value;
           
            if(sfEffectiveDate){
               sfEffectiveDateCheckAction(sfEffectiveDate,effectiveDate); 
            }else{
                    sfEffectiveDateCheckAction(sfEffectiveDate,effectiveDate); 
                 }
           
        };
        
        function nsfEffectiveDateCheck(nsfEffectiveDateId){  
         
           var nsfEffectiveDate = document.getElementById(nsfEffectiveDateId).value;
        
            var effectiveDate = document.getElementById('{!$Component.frm.PB.PBS3.effectiveDateID}').value;
          
            if(nsfEffectiveDate){
               nsfEffectiveDateCheckAction(nsfEffectiveDate,effectiveDate); 
            }else{
                    nsfEffectiveDateCheckAction(nsfEffectiveDate,effectiveDate); 
                 }
           
        };
        
        function gstpEffectiveDateCheck(GSTPEffectiveDateID){  
          
           var gstpEffectiveDate = document.getElementById(GSTPEffectiveDateID).value;
          
            var effectiveDate = document.getElementById('{!$Component.frm.PB.PBS3.effectiveDateID}').value;
          
            if(gstpEffectiveDate){
               gstpEffectiveDateCheckAction(gstpEffectiveDate,effectiveDate); 
            }else{
                    gstpEffectiveDateCheckAction(gstpEffectiveDate,effectiveDate); 
                 }
           
        };
        
         function doCancelterminateDateCheck(){  
          
          if(showTermflag == 'true'){         
           var terminatedDateID = document.getElementById('{!$Component.frm.PB.PBS3.terminatedDateID}');
           var tDay = new Date();
           var tMonth = tDay.getMonth()+1;
            var tDate = tDay.getDate();
            if ( tMonth < 10) tMonth = "0"+tMonth;
            if ( tDate < 10) tDate = "0"+tDate;
            terminatedDateID.value = tMonth+"/"+tDate+"/"+tDay.getFullYear();
            terminatedDateID.disabled = true;
            }
            doCancel();
           
        };
        
        }catch(e){
         
     }
     </script>
